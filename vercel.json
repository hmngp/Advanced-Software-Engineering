{
  "buildCommand": "cd myclean-frontend && npm install && npm run build",
  "outputDirectory": "myclean-frontend/build",
  "devCommand": "cd myclean-frontend && npm start",
  "installCommand": "cd myclean-frontend && npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

